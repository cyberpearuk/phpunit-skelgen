{
    "name": "cyberpearuk/phpunit-skelgen",
    "description": "Tool that can generate skeleton test classes from production code classes and vice versa",
    "keywords": ["phpunit", "xunit", "testing", "skeleton"],
    "homepage": "https://github.com/sebastianbergmann/phpunit-skeleton-generator",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Sebastian Bergmann",
            "email": "sebastian@phpunit.de",
            "role": "lead"
        },
        {
            "name": "James Buncle",
            "email": "jbuncle@hotmail.com",
            "role": "Fork Maintainer"
        }
    ],
    "require": {
        "php": ">=7.1",
        "sebastian/version": "^2.0",
        "phpunit/php-text-template": "^1.2",
        "symfony/console": "^4.3",
        "squizlabs/php_codesniffer": "*"
    },
    "require-dev": {
        "mikey179/vfsstream": "^1.6",
        "overtrue/phplint": "^1.1",
        "phpunit/phpunit": "^7 || ^8.2"
    },
    "autoload": {
        "psr-4": {
            "CyberPear\\PHPUnitSkelGen\\": "src/"
        }
    },
    "bin": [
        "phpunit-skelgen"
    ]
}
